{
	"name": "cxviewer",
	"displayName": "CxViewer",
	"description": "",
	"version": "0.0.1",
	"publisher": "Idan Atzmon",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.scan"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "cxViewer",
					"title": "CxViewer",
					"icon": "images/cxlogo.svg"
				}
			]
		},
		"views": {
			"cxViewer": [
				{
					"id": "mainView",
					"name": "Main View"
				}
			]
		},
		"commands": [
			{
				"command": "extension.scan",
				"title": "Scan with Checkmarx"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.scan"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"@types/jszip": "^3.1.5",
		"jszip": "^3.2.0",
		"superagent": "^4.1.0"
	}
}